{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["warn", 2],
    "semi": "warn",
    "no-console": "off",
    "for-direction": "off",
    "getter-return": "off",
    "no-unused-vars": ["warn", {"args": "none"}],
    "no-var": "warn"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended"
}